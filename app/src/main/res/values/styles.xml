<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RatingBar">
        <item name="colorControlNormal">@color/star_grey</item>
        <item name="colorControlActivated">@color/star_white</item>
    </style>

    <style name="roundedImage">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="Custom.Indicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">@color/lime</item>
    </style>

    <style name="ShapeAppearance.BottomNavigationView.RoundCorners" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:topLeftRadius">30dp</item>
        <item name="android:topRightRadius">30dp</item>
    </style>
</resources>